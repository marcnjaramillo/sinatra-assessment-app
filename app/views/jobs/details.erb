<p> Welcome, <%= current_user.username %>
<h2> Jobs </h2>

<% @jobs.each do |job| %>
  <div>
    <table>
      <tr>
        <th>Location</th>
        <th>Nature</th>
        <th>Duration</th>
      </tr>
      <br>
      <tr>
        <td><%= job.location %></td>
        <td><%= job.nature %></td>
        <td><%= job.duration %> hours</td>
      </tr>
    </table>
  </div>
  <br>
<% end %>
